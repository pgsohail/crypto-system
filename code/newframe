        Encryptor en = Encryptor.getEncrypter(true);
		Decryptor de = Decryptor.getDecrypter(true);
		
		File src = new File(fileName);
		// File dst = new File("C:\\dst");
		en.encrypt(src,src);
    }//GEN-LAST:event_jLabel87MouseClicked

